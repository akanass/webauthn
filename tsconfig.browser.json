{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": "./static/ts",
    "outDir": "./public/js",
    "sourceMap": false,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@simplewebauthn/typescript-types"
    ]
  },
  "files": [
    "static/ts/login.ts",
    "static/ts/webauthn.ts",
    "static/ts/login_authenticator.ts",
    "static/ts/webauthn_authenticator.ts",
    "static/ts/global.ts",
    "static/ts/end.ts",
    "static/ts/error.ts"
  ]
}
